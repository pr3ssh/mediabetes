.header, #footer {
	background: #21b4fa; /*#ee2352; /*#2F2E2E;*/
	color: #D2D2D2;
}
.header{
	clear: both;
	height: 52px;
	line-height: 52px;
	margin-bottom: 40px;
	position: relative;
	text-align: right;
	z-index: 999;
}

#footer{
	height: 20px;
	line-height: 20px;
	position:fixed;
	bottom:0%;
	width:100%;
	text-align: right;
	z-index: 999;
	display:block;
}
.container{
	clear: both;
	margin: 0 auto;
	position: relative;
	width: 890px;
	z-index: 99;
}

.container-footer{
	clear: both;
	margin: 0 auto;
	position: relative;
	width: 535px;
	z-index: 99;
}

#login_form {
	background:#fff; /*rgba(1,170,250,0.8); /*rgba(0,0,0,0.8);*/
	border-radius:10px;
/*	height:220px;
	left:50%;
	margin:-125px 0 0 -250px;*/
	padding:30px 10px 0;
/*	position:absolute;*/
	text-align:center;
	width:480px;
	z-index:99;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	height:auto;
/*	margin-top:0 !important;*/
	padding-bottom:30px;
/*	top:8.5% !important;*/
	margin: 0 auto 65px;
}

#login_form h1, #content h1 {
	color:#000 !important;
	font-family:'Helvetica Neue',Helvetica,Arial,sans-serif;
	font-size:30px;
	font-weight:bold!important;
	margin:0 0 10px!important;
	width:100%;
	padding:10px 0px;
}
#login_form h2 {
	color:#fff!important;
	font-size:17px;
}
#login_form p {
	color:#fff;
	font-size:12px;
	margin:20px 0 0!important;
}
#login_form p small {
	color:#f3f3f3;
	display:block;
	font-size:11px;
	margin-top:4px;
}
#login_form a {
	margin:0 5px;
}
#login_form a .loader {
	position:absolute;
	right:30px;
	top:155px;
}
#login_form form {
	margin:30px auto 0;
	width:404px;
}
#login_form form label {
	color:#000 !important;
	width:80px;
}
#login_form form input, #login_form form .userfile {
	border:none!important;
}
#login_form a {
	color:#fff;
}
#login_form form div.error p{
	float:right;
	width:304px;
	clear:both;
	display:block;
	font-size:11px;
	font-weight:bold;
	margin:-10px auto 20px;
	color:#000;
}

form {
	clear:both;
	overflow:hidden;
}
form label {
	color:#323232;
	font-size:16px;
	font-weight:bold;
	float:left;
	padding:12px 20px 12px 0;
	text-align:right;
	width:280px;
}
form select{
	width:304px !important;
}
form input, form select, form .userfile {
	background:#f2f2f2;
	border:2px solid #bebebe;
	color:#65717c;
	float:left;
	font-size:18px;
	font-weight:bold;
	margin-bottom:20px;
	padding:10px;
	width:280px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
}
form input:focus,form input:hover {
	background:#fbe0e7;
	border:2px solid #a9dff2;
	color:#65717c;
	outline:none;
}
form .checkbox {
	width:auto;
}
form div.checkbox {
	clear:both;
	margin-left:20px;
}
form div.checkbox label,form div.checkbox input.checkbox {
	float:none;
	font-weight:normal;
}
form div.checkbox label {
	color:#455459;
	font-size:13px;
	float:left;
	padding:9px 0 0;
	text-align:left;
	width:520px;
}
form div.checkbox input.checkbox {
	float:left;
	margin:10px 5px 0 0;
}
form input.file {
	background:transparent;
	border:none;
	font-size:13px;
	margin:5px 0;
	padding:0;
	width:auto;
}

form #receta_container{
	display:inline-block;
	margin-bottom:20px;
}
	form #receta_container table{
		width:400px !important;
	}

#signup_form {
	width: 300px;
	background: #f0f0f0 repeat-x 0 0;
	border: 1px solid white;
	margin: 75px auto 0;
	padding: 1em;
	-moz-border-radius: 3px;
}

.bt-small,.bt-big {
	border:none!important;
	color:#fff!important;
	cursor:pointer;
	text-decoration:none!important;
	width:auto;
	border-radius:4px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
}
.bt-red {
	background:rgba(238, 35, 81, 1) !important;
}
.bt-red:hover {
	background:rgba(207, 20, 66, 1)  !important;
}
.bt-blue {
	background:#33b5e4!important;
}
.bt-blue:hover {
	background:#1797c4!important;
}
.bt-orange {
	background:#e84a12!important;
}
.bt-orange:hover,.bt-orange-hover:hover {
	background:#da4600!important;
}
.bt-grey {
	background:#ccc;
}
.bt-grey:hover {
	background:#bebebe;
}
.bt-small {
	font-size:12px;
	padding:8px 10px;
}
.bt-big {
	font-size:16px;
	padding:12px 14px;
}
.bt-caps {
	text-transform:uppercase;
}
.bt-submit {
	margin-top:20px;
	-webkit-appearance: none;
}
.bt-no-margin {
	margin:0!important;
}

.clear {
	clear:both;
	visibility:hidden;
}
.left {
	float:left!important;
}
.right {
	float:right!important;
}

