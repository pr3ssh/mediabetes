.top-menu, .bottom-menu{
	float: left;
}

.top-menu li, .bottom-menu li{
	display: inline;
	position: relative;
}

.top-menu li {
	padding:7px 0px 16px 10px;
}
.top-menu li:hover, .top-menu li.current {
	background:#ee2352;
	padding:7px 0px 16px 10px;
}

.top-menu li a{
	color: #fff;/*#d9d9d9;*/
	margin-right: 20px;
	padding-left: 18px;
	position: relative;
	text-transform: uppercase;
	text-decoration: none;
	outline: none;
	font-weight: bold;
	font-size: 12px;
	background:url(../images/ico.png) no-repeat;
}

.bottom-menu li a{
	color: #FFF;
	margin-right: 20px;
	padding-left: 18px;
	position: relative;
	text-transform: lowercase;
	text-decoration: none;
	outline: none;
	font-weight: bold;
	font-size: 10px;	
}

.top-menu li.inicio a{
	background-position:0px -20px;
}

.top-menu li.add_medida a{
	background-position:0px -86px;
}

.top-menu li.last_medida a{
	background-position:0px -36px;
}

.top-menu li.manage_medida a{
	background-position:0px -52px;
}

.top-menu li.configuracion a{
	background-position:0px 0px;
}

.top-menu li.add_editor a{
	background-position:0px -86px;
}

.top-menu li.salir a{
	background-position:0px -69px;
}

.top-menu li a.current, .top-menu li a.current:hover{
	color: #FFF;
}
.top-menu li a:hover{
	color: #f5f3f4;
}

.top-menu li a.current span.arrow{
	display:block;
	background:url(../images/nav_arrow.png) no-repeat;
	height:10px;
	left:40%;
	position:absolute;
	top:31px;
	width:20px;
}

